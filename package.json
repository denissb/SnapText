{
  "name": "SnapText",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "android": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 13\"",
    "start": "react-native rnx-start",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native rnx-bundle --platform ios --entry-file index.tsx",
    "bundle:android": "react-native rnx-bundle --platform android --entry-file index.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "release": "cd android && ./gradlew bundleRelease",
    "check-dependencies": "yarn rnx-align-deps --requirements react-native@0.76.3",
    "align-deps": "yarn rnx-align-deps --write",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@dr.pogodin/react-native-fs": "^2.30.0",
    "@mgcrea/vision-camera-barcode-scanner": "^0.11.2",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "@react-native-firebase/app": "^21.6.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "i18next": "^24.0.2",
    "react": "18.3.1",
    "react-i18next": "^15.1.2",
    "react-native": "0.76.3",
    "react-native-edge-to-edge": "^1.1.3",
    "react-native-image-crop-picker": "^0.41.6",
    "react-native-modal": "^13.0.0",
    "react-native-reanimated": "^3.16.3",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vision-camera": "4.6.3",
    "react-native-vision-camera-text-recognition": "^3.1.1",
    "react-native-webview": "^13.12.4",
    "react-native-worklets-core": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.2",
    "@react-native/eslint-config": "0.76.2",
    "@react-native/metro-config": "0.76.2",
    "@react-native/typescript-config": "0.76.2",
    "@rnx-kit/align-deps": "^3.0.2",
    "@rnx-kit/cli": "^0.18.3",
    "@rnx-kit/metro-config": "^2.0.1",
    "@rnx-kit/metro-resolver-symlinks": "^0.2.1",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.6.3",
    "prettier": "^3.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "rnx-kit": {
    "bundle": {
      "detectDuplicateDependencies": {
        "ignoredModules": [
          "react-is"
        ]
      },
      "platforms": {
        "ios": {
          "bundleOutput": "ios/main.jsbundle",
          "sourcemapOutput": "ios/main.jsbundle.map"
        },
        "android": {
          "bundleOutput": "android/main.jsbundle",
          "sourcemapOutput": "android/main.jsbundle.map"
        }
      },
      "treeShake": true
    },
    "kitType": "app",
    "typescriptValidation": false,
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "filesystem",
        "jest",
        "modal",
        "react",
        "react-test-renderer",
        "webview"
      ]
    }
  }
}
