{
  "name": "SnapText",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "android": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "start": "react-native rnx-start",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native rnx-bundle --platform ios --entry-file index.tsx",
    "bundle:android": "react-native rnx-bundle --platform android --entry-file index.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "release": "cd android && ./gradlew bundleRelease",
    "postinstall": "npx jetify",
    "check-dependencies": "rnx-dep-check --vigilant 0.65.1",
    "fix-dependencies": "rnx-dep-check --vigilant 0.65.1 --write"
  },
  "dependencies": {
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-firebase/app": "^12.7.5",
    "@types/react-native-vector-icons": "^6.4.11",
    "i18next": "^19.1.0",
    "react": "17.0.2",
    "react-i18next": "^11.11.4",
    "react-native": "^0.66.0",
    "react-native-image-crop-picker": "^0.36.2",
    "react-native-modal": "^13.0.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-vision-camera": "2.13.0",
    "react-native-webview": "^11.13.0",
    "vision-camera-ocr-scanner": "file:.yalc/vision-camera-ocr-scanner"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.0",
    "@rnx-kit/cli": "^0.14.2",
    "@rnx-kit/dep-check": "^1.12.20",
    "@rnx-kit/metro-config": "^1.2.36",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.21",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "4.0.7"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "bundle": {
      "detectDuplicateDependencies": {
        "ignoredModules": [
          "react-is"
        ]
      },
      "platforms": {
        "ios": {
          "bundleOutput": "ios/main.jsbundle",
          "sourcemapOutput": "ios/main.jsbundle.map"
        },
        "android": {
          "bundleOutput": "android/main.jsbundle",
          "sourcemapOutput": "android/main.jsbundle.map"
        }
      },
      "treeShake": true
    },
    "reactNativeVersion": "^0.65",
    "kitType": "app",
    "typescriptValidation": false,
    "capabilities": [
      "babel-preset-react-native",
      "core",
      "core-android",
      "core-ios",
      "jest",
      "modal",
      "react",
      "react-test-renderer",
      "webview"
    ]
  }
}
